# Microburbs API Configuration
# Get your API token from https://www.microburbs.com.au
MICROBURBS_API_TOKEN=test

# Optional: Override API base URL
# MICROBURBS_API_BASE_URL=https://www.microburbs.com.au/report_generator/api

# Optional: Force use of mock data (for testing without API)
# USE_MOCK_DATA=True

# Optional: API request timeout in seconds
# API_TIMEOUT=10

# Optional: Logging level (DEBUG, INFO, WARNING, ERROR)
# LOG_LEVEL=INFO

# Optional: Flask debug mode (should be False in production)
# FLASK_DEBUG=False

# Optional: Application port
# PORT=5001
